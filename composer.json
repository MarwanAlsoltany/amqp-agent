{
    "name": "maks/amqp-agent",
    "type": "library",
    "license": "LGPL-2.1-or-later",
    "description": "An elegant wrapper around the famous php-amqplib for 90% use case.",
    "keywords": [
        "amqp",
        "amqp-agent",
        "php-amqplib",
        "rabbitmq",
        "message-broker",
        "php",
        "async",
        "queues"
    ],
    "authors": [
        {
            "name": "Marwan Al-Soltany",
            "email": "MarwanAlsoltany@gmail.com",
            "homepage": "https://marwanalsoltany.github.io/",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/MarwanAlsoltany/amqp-agent#readme",
    "support": {
        "issues": "https://github.com/MarwanAlsoltany/amqp-agent/issues"
    },
    "require": {
        "php" : "^7.4",
        "php-amqplib/php-amqplib": "^2.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5.5",
        "theseer/phpdox": "^0.12.0",
        "phpunit/phpunit": "~8.5",
        "phploc/phploc": "^5.0.0",
        "phpmd/phpmd": "^2.8.1"
    },
    "autoload": {
        "psr-4": {
            "MAKS\\AmqpAgent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MAKS\\AmqpAgent\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": false,
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
